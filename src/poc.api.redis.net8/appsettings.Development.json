{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "Kafka": {
    "BootstrapServers": "localhost:9092",
    "GroupId": {
      "Produto": "produto-consumer-group"
    },
    "Topic": {
      "Produto": {
        "CriarProduto": "CRIAR_PRODUTO",
        "AlterarProduto": "ALTERAR_PRODUTO",
        "RemoverProduto": "REMOVER_PRODUTO"
      }
    }
  },
  "RabbitMQConnection": {
    "Host": "rabbitmq://localhost",
    "Username": "guest",
    "Password": "guest"
  },
  "Redis": {
    "Configuration": "localhost:6379,password=Poc2Minimal@Api"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Debug",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "MongoDB",
        "Args": {
          "databaseUrl": "mongodb://localhost:27017/admin",
          "collectionName": "ApiLogRedisDev",
          "cappedMaxSizeMb": "1024",
          "cappedMaxDocuments": "50000"
        }
      }
    ]
  }
}
